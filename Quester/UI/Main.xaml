<controls:MetroWindow  x:Name="Main1" x:Class="WowAI.UI.Main"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
             xmlns:local="clr-namespace:WowAI"
             mc:Ignorable="d" 
             Background="#FF3E4146" Closing="MetroWindow_Closing" SizeChanged="Main1_SizeChanged" Loaded="Main1_Loaded" Width="1700" Height="800">
    <controls:MetroWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../AppStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </controls:MetroWindow.Resources>

    <Grid>
        <Grid Background="#FF36393E"   VerticalAlignment="Top" Width="300" HorizontalAlignment="Left">
            <StackPanel>
                <WrapPanel >
                    <Menu
                        Foreground="White" 
                        Background="#FF36393E">
                        <MenuItem
                            Header="Настройки" 
                            Click="SubItem1_Click" 
                            HorizontalAlignment="Left" 
                            FontSize="12" 
                            Background="#FF4A4F55" 
                            Margin="0"/>
                    </Menu>
                    <WrapPanel Margin="5,0,0,0">
                        <Button Content="Load" Click="ButtonLoadSet_Click"  />
                        <Button Content="Save" Click="ButtonSaveSet_Click"  Margin="5,0,0,0" />
                    </WrapPanel>
                    <ComboBox x:Name="ComboBoxSwitchMode"  SelectionChanged="ComboBoxSwitchMode_SelectionChanged" Margin="5,0,0,0" Width="138" Height="Auto">
                        <ComboBoxItem Content="Квестинг"/>
                        <ComboBoxItem Content="Фарм мобов" />
                        <ComboBoxItem Content="Фарм ресурсов" />
                        <ComboBoxItem Content="Скрипт" />
                        <ComboBoxItem Content="Отбивание" />
                        <ComboBoxItem Content="Квестинг Классик" />
                    </ComboBox>

                </WrapPanel>
                <Label x:Name="LabelProfile" Content="Профиль" Margin="0,-10,0,0"/>
                <Label x:Name="LabelGameState" Content="GameState:" Margin="0,-10,0,0" />
                <Label x:Name="MeTarget" Content="Цель: " Margin="0,-10,0,0" Foreground="White"/>
                <controls:MetroProgressBar 
                                    x:Name="ProgressBarTargetHp" 
                                    Margin="5,-4,5,0" 
                                    VerticalAlignment="Stretch" Height="15" 
                                    Foreground="#FFD81313" BorderBrush="#FF4A4F55" Background="#FF4A4F55" />
                <TextBlock x:Name="TextBlockTargetHp" HorizontalAlignment="Center" Margin="0,-15,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Text="{Binding Value, ElementName=ProgressBarTargetHp}" RenderTransformOrigin="0.769,6.188" Foreground="White"/>

                <Label x:Name="BestMob" Content="BestMob: " Margin="0,-10,0,0"/>
                <Label x:Name="BestProp" Content="BestProp:"  Margin="0,-10,0,0"/>
                <Label x:Name="FarmState" Content="FarmState: null"  Margin="0,-10,0,0" />
                <Label x:Name="QuestId" Content="QuestId: null" Margin="0,-10,0,0" />
                <Label x:Name="QuestState" Content="QuestState: null" Margin="0,-10,0,0" />
                <Label x:Name="IsMoveToNow" Content="isMoveToNow:null" Margin="0,-10,0,0" />
                <Label x:Name="LabelTimeWork" Content="Время работы:" Margin="0,-10,0,0" />
                <Label x:Name="LabelKillMobs" Content="Убито мобов:" Margin="0,-10,0,0" />
                <Label x:Name="LabelGold" Content="Золото:" Margin="0,-10,0,0" />
                <Label x:Name="LabelGoldInHour" Content="Золото в час/день:" Margin="0,-10,0,0" />
                <Label x:Name="LabelExp" Content="Опыт:" Margin="0,-10,0,0" />
                <controls:MetroProgressBar  x:Name="ProgressBarMeHp" 
                                    Margin="5,-3,5,0" 
                                    VerticalAlignment="Stretch" 
                                    Height="15"                                       
                                    Foreground="#FFD81313" Background="#FF4A4F55" />
                <TextBlock x:Name="TextBlockHp" HorizontalAlignment="Center" Margin="1,-15,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Text="{Binding Value, ElementName=ProgressBarMeHp}" Foreground="White"/>
                <controls:MetroProgressBar
                                    x:Name="ProgressBarMeMp" 
                                    Margin="5,0"
                                    VerticalAlignment="Stretch"
                                    Height="15" Background="#FF4A4F55" />
                <TextBlock x:Name="TextBlockMp" HorizontalAlignment="Center" Margin="0,-15,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Text="{Binding Value, ElementName=ProgressBarMeMp}" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}"/>
                <controls:MetroProgressBar  x:Name="ProgressBarMeEnergy" Margin="5,0" VerticalAlignment="Stretch" Height="15" Background="#FF4A4F55" Value="100" Foreground="#FF99B4D1" />
                <TextBlock x:Name="TextBlockMeEnergy" HorizontalAlignment="Center" Margin="0,-15,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Text="{Binding Value, ElementName=ProgressBarMeEnergy}" Foreground="White" OpacityMask="#FF36393E"/>
                <TextBox x:Name="TextboxMeCoord" Margin="5,0,0,0" TextWrapping="Wrap" Text="TextBox" Background="#FF36393E" BorderBrush="#FF4A4F55" Foreground="White"/>
                <Grid Margin="0,5,0,0">
                    <Button Content="Сброс стат" HorizontalAlignment="Right" Margin="0,0,0,10" VerticalAlignment="Bottom" Click="ButtonResetStat_Click" RenderTransformOrigin="-2.107,2.12"
                            Background="#FF36393E"  Panel.ZIndex="1"/>
                    <Button x:Name="ButtonOnOff" Content="Выключить" HorizontalAlignment="Center" Margin="112,0,107,10" VerticalAlignment="Bottom" Click="ButtonOnOff_Click" RenderTransformOrigin="-2.107,2.12"
                            Background="#FF36393E"  Panel.ZIndex="1"/>

                    <Button x:Name="ButtonContinue" Content="Продолжить" Margin="0,0,0,9" VerticalAlignment="Bottom" Click="ButtonContinue_Click" RenderTransformOrigin="-2.107,2.12"
                            Background="#FF36393E" IsEnabled="False" HorizontalAlignment="Left"/>
                </Grid>
                <ComboBox x:Name="ComboBoxDungeonScript" SelectedIndex="0" PreviewMouseLeftButtonDown="ComboBoxDungeonScript_PreviewMouseLeftButtonDown">
                    <ComboBoxItem Content="Не выбрано"/>
                </ComboBox>

            </StackPanel>
        </Grid>

        <Grid Margin="300,0,0,0" Background="#FF36393E" Panel.ZIndex="2">
            <TabControl Margin="0" Grid.Column="0" Grid.Row="0" SelectedIndex="0">
                <TabItem Header="Настройки">
                    <StackPanel Background="#FF36393E" Margin="0" >
                        <WrapPanel Orientation="Horizontal">
                            <GroupBox Header="Общее" Margin="0">
                                <StackPanel>
                                    <WrapPanel>
                                        <CheckBox x:Name="CheckBoxAutoEquip" IsChecked="{Binding AutoEquip}" Content="Автоэкипировка"/>
                                        <TextBox x:Name="TextBoxEquipStateWeapon" Text="0" />
                                    </WrapPanel>
                                    <CheckBox x:Name="CheckBoxNoAttackOnMount" Content="Не отбиваться на маунте"  IsChecked="{Binding NoAttackOnMount}"/>
                                    <CheckBox x:Name="CheckBoxUnmountMoveFail" Content="Слезать с маунта при застревании"  IsChecked="True"/>
                                    <CheckBox x:Name="CheckBoxAttackForSitMount" Content="Отбиваться если нужно сесть на маунта"   IsChecked="{Binding NoAttackOnMount}"/>
                                    <CheckBox x:Name="CheckBoxUseDash" Content="Использовать ускорение" IsChecked="{Binding UseDash}"/>
                                    <CheckBox x:Name="CheckBoxPickUpLoot" Content="Подбирать лут" IsChecked="True"/>
                                    <WrapPanel>
                                        <Label Content="Игнорировать моба через"/>
                                        <TextBox x:Name="TextBoxIgnoreMob"  Text="20000" />
                                        <Label Content="мс"/>
                                    </WrapPanel>
                                    <WrapPanel>
                                        <CheckBox x:Name="CheckBoxUseRegen" Content="Реген когда ХП &lt;" />
                                        <TextBox x:Name="TextBoxHpRegen"  Text="85"  />
                                        <Label Content="МП &lt;"/>
                                        <TextBox x:Name="TextBoxMpRegen"  Text="85" />
                                        <Label Content="Пет &lt;"/>
                                        <TextBox x:Name="TextBoxPetRegen" Text="85"/>
                                    </WrapPanel>
                                    <CheckBox x:Name="CheckBoxPvp" Content="Отбиваться в ПВП"  Margin="0,0,-158,0"  />
                                    <CheckBox x:Name="CheckBoxUseFly" Content="Использовать перелеты"  IsChecked="True" Margin="0,0,-158,0"/>
                                    <CheckBox x:Name="CheckBoxKillRunaways" Content="Добивать убегающих мобов"  IsChecked="True" Margin="0,0,-158,0"/>
                                    <CheckBox x:Name="CheckBoxKillMobsFirst" Content="Нападать на мобов первым" Margin="0,0,-158,0" />
                                    <CheckBox x:Name="CheckBoxRandomJump" Content="Рандомные прыжки" Margin="0,0,-158,0" />
                                    <CheckBox x:Name="CheckBoxUseStoneIsStuck" Content="Использовать камень при застревании"  IsChecked="True" Margin="0,0,-158,0"/>
                                    <CheckBox x:Name="CheckBoxAttackMobForDrop" Content="Убивать мобов, которые мешают при подборе дропа"  IsChecked="True" Margin="0,0,-158,0"/>
                                    <WrapPanel Margin="0" >
                                        <CheckBox x:Name="CheckBoxRegisterStone" Content="Рег. камень"/>
                                        <TextBox x:Name="TextBoxStoneLocX" TextWrapping="Wrap" Text="0" />
                                        <TextBox x:Name="TextBoxStoneLocY" TextWrapping="Wrap" Text="0"  />
                                        <TextBox x:Name="TextBoxStoneLocZ" TextWrapping="Wrap" Text="0"  />
                                        <Label Content="Map" Margin="0" Background="#00000000" Foreground="White"/>
                                        <TextBox x:Name="TextBoxStoneLocMapId" TextWrapping="Wrap" Text="0"  />
                                        <Label Content="Area" Margin="0" Background="#00000000" Foreground="White"/>
                                        <TextBox x:Name="TextBoxStoneLocAreaId" TextWrapping="Wrap" Text="0"  />
                                        <Button Content="XYZ" Margin="10,0,0,0" Click="ButtonStoneLocFill_Click" />
                                    </WrapPanel>
                                    <WrapPanel Margin="0,0,-158,0">
                                        <CheckBox x:Name="CheckBoxRunRun" Content="Пытаться убежать, если шанс смерти больше"/>
                                        <TextBox x:Name="TextBoxRunRunPercent" Text="80" />
                                    </WrapPanel>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox Header="Квестинг" Margin="0">
                                <Grid Margin="0">

                                    <StackPanel Margin="0" Orientation="Vertical" VerticalAlignment="Top">
                                        <ComboBox x:Name="ComboBoxQuestSet" VerticalAlignment="Top" SelectedIndex="0" PreviewMouseLeftButtonDown="ComboBoxQuestSet_PreviewMouseLeftButtonDown" Margin="0,0,150,0" Height="Auto">
                                            <ComboBoxItem Content="Не выбрано"/>
                                        </ComboBox>
                                        <WrapPanel>
                                            <CheckBox x:Name="CheckBoxStopQuesting" Content="Выключать квестинг по достижению "   IsChecked="{Binding StopQuesting}"/>
                                            <TextBox x:Name="TextBoxStopQuestingLevel" Text="12" />
                                            <Label Content="уровня"/>
                                        </WrapPanel>
                                        <CheckBox x:Name="CheckBoxDebuffDeath" Content="Ожидать дебаф смерти" IsChecked="True"/>
                                        <CheckBox x:Name="CheckBoxLaunchSkript" Content="Запустить скрипт по завершению квестов" />
                                        <CheckBox x:Name="CheckBoxWorldQuest" Content="Мировые квесты"  IsChecked="True"/>
                                        <CheckBox x:Name="CheckBoxAdvancedFight" Content="Улучшенная боевка"/>
                                        <CheckBox x:Name="CheckBoxLearnAllSpell" Content="Изучать все скилы"/>
                                    </StackPanel>
                                    <StackPanel Margin="0" VerticalAlignment="Bottom">
                                        <CheckBox x:Name="CheckBoxLogScript" Content="Лог режима скрипт(Действия)"  />
                                        <CheckBox x:Name="CheckBoxLogSkill" Content="Лог использования скилов" IsChecked="{Binding LogSkill}"/>
                                        <CheckBox x:Name="CheckBoxAllLog" Content="Все логи"/>
                                    </StackPanel>
                                </Grid>
                            </GroupBox>
                            <GroupBox Header="Игнор квестов" Margin="0" Width="250">
                                <StackPanel>
                                    <DataGrid ItemsSource="{Binding CollectionIgnoreQuest}" CanUserAddRows="False" Height="250"/>
                                    <StackPanel>
                                        <TextBox x:Name="TextBoxIgnoreQuestId" Text="Id" HorizontalAlignment="Left" />
                                        <Button Content="Добавить" Click="ButtonIgnoreQuestAdd_Click" HorizontalAlignment="Left" Margin="5,0,0,0"/>
                                    </StackPanel>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox Header="Список квестов" Margin="0" Visibility="Collapsed">
                                <Grid Margin="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="481*"/>
                                        <ColumnDefinition Width="596*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="462*"/>
                                        <RowDefinition Height="215*"/>
                                    </Grid.RowDefinitions>
                                    <StackPanel Grid.ColumnSpan="2" Margin="0" Orientation="Vertical" Grid.RowSpan="2">
                                        <ListView x:Name="ListViewQuest" ItemsSource="{Binding CollectionQuestSettings}" Height="200" Margin="0,5,0,0">
                                            <ListView.View>
                                                <GridView>
                                                    <GridViewColumn Header="Run" >
                                                        <GridViewColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <!-- ReSharper disable once Xaml.BindingWithContextNotResolved -->
                                                                <CheckBox Content="" IsChecked="{Binding Run}"/>
                                                            </DataTemplate>
                                                        </GridViewColumn.CellTemplate>
                                                    </GridViewColumn>
                                                    <GridViewColumn Header="QuestId" DisplayMemberBinding="{Binding QuestId}" Width="80"/>
                                                    <GridViewColumn Header="QuestName" DisplayMemberBinding="{Binding QuestName}" Width="280"/>
                                                    <GridViewColumn Header="NpcId" DisplayMemberBinding="{Binding NpcId}" Width="80"/>
                                                    <GridViewColumn Header="Loc" DisplayMemberBinding="{Binding Loc}" Width="150"/>
                                                    <GridViewColumn Header="State" DisplayMemberBinding="{Binding State}" Width="80"/>
                                                    <GridViewColumn Header="Level" DisplayMemberBinding="{Binding Level}" Width="80"/>
                                                    <GridViewColumn Header="MinLevel" DisplayMemberBinding="{Binding MinLevel}" Width="80"/>

                                                </GridView>
                                            </ListView.View>
                                        </ListView>
                                        <StackPanel Margin="5,5,0,0" Orientation="Horizontal">
                                            <Label Content="QuestId" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0" Background="#00000000" Foreground="White"/>
                                            <TextBox TextWrapping="Wrap" Text="{Binding MountLocX, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Width="59"/>
                                            <Label Content="NpcId" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0" Background="#00000000" Foreground="White"/>
                                            <TextBox TextWrapping="Wrap" Text="{Binding MountLocX, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Width="59" Margin="5,0,-5,0"/>
                                            <Button Content="&gt;" RenderTransformOrigin="0.5,0.5"
                                                controls:ControlsHelper.ContentCharacterCasing="Lower" FontSize="8" Width="15" Background="#FF36393E"
                                                Click="ButtonUpQuest_Click" VerticalAlignment="Top" HorizontalAlignment="Left" Height="25"
                                                Margin="80,0,0,0">
                                                <Button.RenderTransform>
                                                    <TransformGroup>
                                                        <ScaleTransform/>
                                                        <SkewTransform/>
                                                        <RotateTransform Angle="-90"/>
                                                        <TranslateTransform/>
                                                    </TransformGroup>
                                                </Button.RenderTransform>
                                            </Button>
                                            <Button Content="&lt;" RenderTransformOrigin="0.5,0.5"
                                                controls:ControlsHelper.ContentCharacterCasing="Lower" FontSize="8" Width="15"
                                                Background="#FF36393E" Click="ButtonDownQuest_Click" VerticalAlignment="Top" HorizontalAlignment="Left" Height="25"
                                                Margin="20,0,0,0">
                                                <Button.RenderTransform>
                                                    <TransformGroup>
                                                        <ScaleTransform/>
                                                        <SkewTransform/>
                                                        <RotateTransform Angle="-90"/>
                                                        <TranslateTransform/>
                                                    </TransformGroup>
                                                </Button.RenderTransform>
                                            </Button>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,5,0,0">
                                            <Button Content="Добавить" Margin="5,0,0,0" Click="Button4_Click"/>
                                            <Button Content="Удалить" Margin="10,0,0,0"/>
                                            <Button Content="Сохранить" Margin="5,0,0,0" Click="ButtonSaveQuest_Click"/>
                                        </StackPanel>
                                    </StackPanel>
                                </Grid>
                            </GroupBox>
                            <GroupBox Header="Настройки маунта" Margin="0" Width="250">
                                <StackPanel>
                                    <DataGrid ItemsSource="{Binding CollectionPetSettings}" CanUserAddRows="False" Height="250"/>
                                    <StackPanel>
                                        <ComboBox x:Name="ComboBoxInvItems1" PreviewMouseLeftButtonDown="ComboBoxInvItems1_PreviewMouseLeftButtonDown" Width="220" HorizontalAlignment="Left"/>
                                        <Button Content="Добавить" Click="Button3_Click" HorizontalAlignment="Left"/>
                                    </StackPanel>
                                </StackPanel>
                            </GroupBox>
                        </WrapPanel>
                        <WrapPanel>
                            <GroupBox Header="Настройки классов" Margin="0">
                                <StackPanel>
                                    <WrapPanel>
                                        <CheckBox x:Name="CheckBoxSummonBattlePet" Content="Призывать боевого питомца" />
                                        <ComboBox x:Name="ComboBoxSummonBattlePetNumber" Width="150" SelectedIndex="0">
                                            <ComboBoxItem Content="Пет 1"/>
                                            <ComboBoxItem Content="Пет 2"/>
                                            <ComboBoxItem Content="Пет 3"/>
                                            <ComboBoxItem Content="Пет 4"/>
                                            <ComboBoxItem Content="Пет 5"/>
                                        </ComboBox>
                                    </WrapPanel>
                                    <WrapPanel>
                                        <CheckBox x:Name="CheckBoxResPetInCombat" Content="Воскрешать питомца в бою. Mp персонажа &gt;" IsChecked="True"/>
                                        <TextBox x:Name="TextBoxResPetMeMp" Text="90" />
                                    </WrapPanel>
                                    <WrapPanel>
                                        <Label Content="Форма для боя "/>
                                        <ComboBox x:Name="ComboBoxFormForFight" SelectedIndex="0" Width="150" SelectionChanged="ComboBoxSwitchMode_SelectionChanged">
                                            <ComboBoxItem Content="Не использовать" />
                                            <ComboBoxItem Content="Облик медведя"/>
                                            <ComboBoxItem Content="Облик кошки" />
                                            <ComboBoxItem Content="Облик лунного совуха" />
                                        </ComboBox>
                                    </WrapPanel>
                                    <WrapPanel>
                                        <Label Content="Форма для бега"/>
                                        <ComboBox x:Name="ComboBoxFormForMove" SelectedIndex="0" Width="150" SelectionChanged="ComboBoxSwitchMode_SelectionChanged">
                                            <ComboBoxItem Content="Не использовать" />
                                            <ComboBoxItem Content="Облик медведя"/>
                                            <ComboBoxItem Content="Облик кошки" />
                                            <ComboBoxItem Content="Походный облик"/>
                                        </ComboBox>
                                    </WrapPanel>
                                    <WrapPanel>
                                        <CheckBox x:Name="CheckBoxCraftConjured" Content="Крафтить и использовать бутылки магом HP &lt;" IsChecked="True"/>
                                        <TextBox x:Name="TextBoxCraftConjuredHp" Text="80" />
                                        <Label Content="Mp &lt;"/>
                                        <TextBox x:Name="TextBoxCraftConjuredMp"  Text="80" />
                                    </WrapPanel>
                                    <WrapPanel>
                                        <CheckBox x:Name="CheckBoxUseArrow" Content="Использовать указанные стрелы"/>
                                        <TextBox x:Name="TextBoxUseArrowId"  Text="0" />
                                    </WrapPanel>
                                    <CheckBox x:Name="CheckBoxLearnPetSpell" Content="Изучать скилы питомца охотника"/>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox Header="Открывать TaxyNode" Margin="0">
                                <StackPanel>
                                    <DataGrid ItemsSource="{Binding CollectionTaxyNodeSettingses}" CanUserAddRows="False" Height="250"/>
                                    <StackPanel>
                                        <ComboBox x:Name="ComboBoxTaxyNode" PreviewMouseLeftButtonDown="ComboBoxTaxyNode_PreviewMouseLeftButtonDown" Width="220" HorizontalAlignment="Left"/>
                                        <Button Content="Добавить" Click="Button_Click_2" HorizontalAlignment="Left"/>
                                    </StackPanel>
                                </StackPanel>
                            </GroupBox>
                        </WrapPanel>
                    </StackPanel>
                </TabItem>
                <TabItem Header="Умения">
                    <TabControl>
                        <TabItem Header="Атакующие">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="319*"/>
                                    <RowDefinition Height="360*"/>
                                </Grid.RowDefinitions>

                                <DockPanel >
                                    <GroupBox Header="Доступные умения" Margin="0">
                                        <DataGrid x:Name="DataGridAllSkill" Margin="0" ItemsSource="{Binding CollectionAllSkill}" CanUserAddRows="False" SelectionChanged="ListViewAllSkill_SelectionChanged" />
                                    </GroupBox>
                                    <GroupBox Header="Активные скилы" controls:ControlsHelper.ContentCharacterCasing="Upper" Margin="0">
                                        <Grid>
                                            <DataGrid x:Name="DataGridActiveSkill" ItemsSource="{Binding CollectionActiveSkill}" CanUserAddRows="False" SelectionChanged="ListViewActiveSkill_SelectionChanged" />
                                        </Grid>
                                    </GroupBox>
                                </DockPanel>
                                <GroupBox x:Name="GroupBoxSettingsSkill" Header="Настройки" Grid.Row="1"
                                          Grid.Column="0" Margin="0">
                                    <StackPanel >
                                        <WrapPanel Margin="0,0,0,5">
                                            <Button  x:Name="ButtonAddSkill" IsEnabled="False" Content="Добавить" Click="ButtonAddSkill_Click" />
                                            <Button  x:Name="ButtonDelSkill" IsEnabled="False" Content="Удалить" Click="ButtonDelSkill_Click" Margin="10,0,0,0" />
                                            <Button  x:Name="ButtonChangeSkill" Margin="10,0,0,0" IsEnabled="False" Content="Редактировать" Click="ButtonChangeSkill_Click" />
                                            <Button Content="Авто добавление скилов"  Click="ButtonAddSkillAuto_Click" Margin="30,0,5,0"/>
                                            <CheckBox x:Name="CheckBoxRandomDistForAttack" Content="Начинать атаку с рандомной дистанции. MaxDist -"/>
                                            <TextBox x:Name="TextBoxRandomDistForAttackCount" Text="10" PreviewTextInput="TextBoxCharacterMinHp_PreviewTextInput"/>
                                        </WrapPanel>
                                        <WrapPanel>
                                            <StackPanel>
                                                <GroupBox Header="Приоритет:">
                                                    <Grid>
                                                        <TextBox x:Name="TextBoxPriority" TextWrapping="Wrap" Text="0" PreviewTextInput="TextBox_PreviewTextInput" VerticalAlignment="Top" HorizontalAlignment="Left" />
                                                        <Button Content="&gt;"  HorizontalAlignment="Right" Margin="0,-7,5,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5"
                                                                controls:ControlsHelper.ContentCharacterCasing="Lower" FontSize="8" Width="15" Background="#FF36393E"
                                                                Click="ButtonUpPriotity_Click" >
                                                            <Button.RenderTransform>
                                                                <TransformGroup>
                                                                    <ScaleTransform/>
                                                                    <SkewTransform/>
                                                                    <RotateTransform Angle="-90"/>
                                                                    <TranslateTransform/>
                                                                </TransformGroup>
                                                            </Button.RenderTransform>
                                                        </Button>
                                                        <Button Content="&lt;" HorizontalAlignment="Right" Margin="0,8,5,0" VerticalAlignment="Bottom" RenderTransformOrigin="0.5,0.5"
                                                                controls:ControlsHelper.ContentCharacterCasing="Lower" FontSize="8" Width="15"
                                                                Background="#FF36393E" Click="ButtonDownPriority_Click" >
                                                            <Button.RenderTransform>
                                                                <TransformGroup>
                                                                    <ScaleTransform/>
                                                                    <SkewTransform/>
                                                                    <RotateTransform Angle="-90"/>
                                                                    <TranslateTransform/>
                                                                </TransformGroup>
                                                            </Button.RenderTransform>
                                                        </Button>
                                                    </Grid>
                                                </GroupBox>
                                                <CheckBox x:Name="CheckBoxSelfTarget" Content="На себя" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" BorderBrush="#FF4A4F55" Background="#FF36393E" Height="18" Width="67" />
                                                <CheckBox x:Name="CheckBoxUseInFight" Content="В бою" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" BorderBrush="#FF4A4F55" Background="#FF36393E" Height="18" Width="67"  IsChecked="True"/>
                                            </StackPanel>
                                            <GroupBox Header="Персонаж:" controls:ControlsHelper.ContentCharacterCasing="Normal" controls:ControlsHelper.HeaderFontSize="12">
                                                <Grid Margin="0">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="141*"/>
                                                        <ColumnDefinition Width="184*"/>
                                                        <ColumnDefinition />
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="34*" MaxHeight="50"/>
                                                        <RowDefinition Height="37*"/>
                                                    </Grid.RowDefinitions>
                                                    <WrapPanel Margin="0">

                                                        <Label Content="MinHP &gt;" Foreground="White" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0"/>
                                                        <Grid Height="34" Width="90">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="22*"/>
                                                                <ColumnDefinition Width="23*"/>
                                                            </Grid.ColumnDefinitions>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition/>
